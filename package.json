    "recharts": "^3.1.0",
    "sonner": "^2.0.6",
    "ts-buildkit": "^1.1.0",
    "unified-error-handling": "^1.2.0",
    "unified-tracking": "^1.5.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
