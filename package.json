{
  "name": "labflow",
  "displayName": "LabFlow",
  "description": "Comprehensive multi-tenant laboratory management system for clinical laboratories with patient management, test tracking, results reporting, billing, inventory management, quality control, and EMR integration.",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "author": {
    "name": "Ahsan Mahmood",
    "email": "aoneahsan@gmail.com",
    "url": "https://aoneahsan.com"
  },
  "license": "MIT",
  "homepage": "https://labflow.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/aoneahsan/labflow.git"
  },
  "bugs": {
    "url": "https://github.com/aoneahsan/labflow/issues"
  },
  "keywords": [
    "laboratory",
    "management",
    "LIMS",
    "clinical",
    "medical",
    "healthcare",
    "EMR",
    "patient-management",
    "test-tracking",
    "lab-automation",
    "multi-tenant",
    "react",
    "capacitor",
    "firebase"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --config vite.config.prod.ts",
    "build:prod:notsc": "vite build --config vite.config.prod.simple.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "preview": "vite preview",
    "preview:prod": "vite preview --config vite.config.prod.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,css,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,css,md}'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:prod": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cap:sync": "cap sync",
    "cap:build:android": "cap build android",
    "cap:build:ios": "cap build ios",
    "seed:demo": "node scripts/seed-demo-tenant.mjs",
    "seed:demo:admin": "node scripts/create-demo-tenant-admin.mjs",
    "generate:demo": "node scripts/generateDemoData.js",
    "docker:build": "docker build -t labflow:latest .",
    "docker:run": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:stop": "docker-compose -f docker-compose.prod.yml down",
    "deploy:check": "yarn lint && yarn typecheck && yarn test:prod"
  },
  "dependencies": {
    "@capacitor-community/speech-recognition": "^7.0.1",
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.1",
    "@capacitor/browser": "^7.0.1",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/device": "^7.0.1",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/geolocation": "^7.1.4",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/local-notifications": "^7.0.1",
    "@capacitor/network": "^7.0.1",
    "@capacitor/push-notifications": "^7.0.1",
    "@capacitor/share": "^7.0.1",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-switch": "^1.2.5",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/jspdf": "^2.0.0",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.11.0",
    "buildkit-ui": "^1.3.0",
    "capacitor-auth-manager": "^2.1.0",
    "capacitor-biometric-authentication": "^2.0.0",
    "capacitor-firebase-kit": "^2.0.1",
    "capacitor-native-update": "^2.0.0",
    "chart.js": "^4.5.0",
    "code-craft-studio": "^2.0.0",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.11",
    "file-saver": "^2.0.5",
    "firebase": "^12.0.0",
    "jspdf": "^3.0.1",
