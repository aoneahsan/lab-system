    "capacitor-auth-manager": "^1.1.0",
    "capacitor-biometric-authentication": "^1.4.3",
    "capacitor-firebase-kit": "^1.1.0",
    "capacitor-native-update": "^1.4.0",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "firebase": "^12.0.0",
    "jsbarcode": "^3.12.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.525.0",
    "notification-kit": "^1.2.0",
    "qrcode-studio": "^1.1.0",
